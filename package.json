{
  "name": "veeggo-app",
  "version": "1.0.0",
  "description": "Veeggo App",
  "main": "index.js",
  "engines": {
    "npm": ">=5",
    "node": ">=6"
  },
  "scripts": {
    "build": "node ./tools/scripts/build.js",
    "build:temp": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:dll": "node ./tools/scripts/dependencies.js",
    "build:clean": "rimraf ./build",
    "clean": "shjs ./tools/scripts/clean.js",
    "clean:dll": "rimraf ./node_modules/react-dll",
    "extract-intl": "node -- ./tools/scripts/extract-intl.js",
    "generate": "plop --plopfile ./tools/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern tools/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "npmcheckversion": "node ./tools/scripts/npmcheckversion.js",
    "prebuild": "npm run build:clean",
    "preinstall": "npm run npmcheckversion",
    "prestart": "npm run clean:dll && npm run build:dll",
    "start": "node ./tools/scripts/start.js"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "dllPlugin": {
    "path": "node_modules/react-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "author": "Alberto Arana <@aaranaboza>",
  "license": "MIT",
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.3",
    "autoprefixer": "8.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chalk": "2.3.2",
    "circular-dependency-plugin": "4.4.0",
    "compression": "1.7.2",
    "cross-env": "5.1.4",
    "css-loader": "0.28.10",
    "detect-port-alt": "1.1.5",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-redux-saga": "0.6.0",
    "eventsource-polyfill": "0.9.6",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.11",
    "filesize": "3.6.0",
    "flow-bin": "0.68.0",
    "fs-extra": "^5.0.0",
    "gzip-size": "4.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.0.6",
    "imports-loader": "0.8.0",
    "inquirer": "5.1.0",
    "ip": "1.1.5",
    "is-root": "1.0.0",
    "minimist": "1.2.0",
    "ngrok": "3.0.0",
    "offline-plugin": "4.9.0",
    "plop": "2.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-media": "6.0.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-global-import": "1.0.6",
    "postcss-import": "^11.1.0",
    "postcss-loader": "2.1.2",
    "postcss-nested": "3.0.0",
    "recursive-readdir": "2.2.2",
    "rimraf": "2.6.2",
    "shelljs": "0.8.1",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.11",
    "webpack-dev-middleware": "3.0.1",
    "webpack-hot-middleware": "2.21.2"
  },
  "dependencies": {
    "fontfaceobserver": "2.0.13",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.5.0",
    "immutable": "3.8.2",
    "intl": "1.2.5",
    "lodash": "4.17.5",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "sanitize.css": "5.0.0",
    "styled-components": "3.2.1",
    "whatwg-fetch": "2.0.3"
  }
}
